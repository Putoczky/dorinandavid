{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "src/**/*"
  ],
  "exclude": [
    "dist",
    "../worker"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@worker/*": ["../worker/*"],
      "@app/*": ["src/*"]
    }
  },
  "references": [
    {
      "path": "../worker"
    }
  ]
}